</div></div></div><div class="w-layout-blockcontainer container_s w-container"><div class="w-embed w-script"><script>// Function to get the current page title and URL
function getPageInfo() {
  return {
    title: document.title,
    url: window.location.href
  };
}
// Function to create the share URL
function createShareUrl(base, params) {
  const url = new URL(base);
  for (const [key, value] of Object.entries(params)) {
    url.searchParams.append(key, value);
  }
  return url.toString();
}
// Set up the share buttons
document.addEventListener('DOMContentLoaded', () => {
  const pageInfo = getPageInfo();
  const twitterUrl = createShareUrl('http://twitter.com/share', {
    text: pageInfo.title,
    url: pageInfo.url
  });
  const linkedinUrl = createShareUrl('http://www.linkedin.com/shareArticle', {
    url: pageInfo.url
  });
  const facebookUrl = createShareUrl('https://www.facebook.com/sharer/sharer.php', {
    u: pageInfo.url
  });
  document.getElementById('twitter-share').addEventListener('click', () => {
    window.open(twitterUrl, '_blank');
  });
  document.getElementById('linkedin-share').addEventListener('click', () => {
    window.open(linkedinUrl, '_blank');
  });
  document.getElementById('facebook-share').addEventListener('click', () => {
    window.open(facebookUrl, '_blank');
  });
});
</script></div><div class="blog_container is-article"><div li-object="article.content" class="blog_richtext w-richtext">{{ article.content }}</div><div id="w-node-_838977cc-c4b9-4089-8d04-f6fc1df39205-07f8c28e" class="blog_post-share"><h2 class="share_label">Share post</h2><hr class="divider" style="background-color:#2A5FF9">
